{
  "samba-container-config": "v0",
  "configs": {
    "SAMBA": {
      "shares": [
        "luca-timemachine",
        "homeshare",
        "public"
      ],
      "globals": [
        "default"
      ],
      "instance_name": "SAMBA"
    }
  },
  "shares": {
    "luca-timemachine": {
      "options": {
        "path": "/luca-timemachine",
        "valid users": "luca",
        "writeable": "true",
        "fruit:time machine": "yes"
      }
    },
    "home": {
      "options": {
        "path": "/homes/%U",
        "valid users": "luca",
        "writeable": "true",
        "root preexec": "mkdir /homes/%U"
      }
    },
    "public": {
      "options": {
        "path": "/public",
        "public": "yes",
        "writeable": "true"
      }
    },
    "paperless-ingest": {
      "options": {
        "path": "/paperless-ingest",
        "public": "yes",
        "writeable": "true",
        "valid users": "luca printer"
      }
    }
  },
  "globals": {
    "default": {
      "options": {
        "security": "user",
        "server min protocol": "SMB2",
        "load printers": "no",
        "printing": "bsd",
        "printcap name": "/dev/null",
        "disable spoolss": "yes",
        "guest ok": "no",
        "vfs objects": "fruit streams_xattr",
        "fruit:metadata": "stream",
        "fruit:model": "MacSamba",
        "fruit:posix_rename": "yes",
        "fruit:veto_appledouble": "no",
        "fruit:nfs_aces": "no",
        "fruit:wipe_intentionally_left_blank_rfork": "yes",
        "fruit:delete_empty_adfiles": "yes"
      }
    }
  },
  "users": {
    "all_entries": [
      {
        "name": "luca",
        "password": "test"
      },
      {
        "name": "printer",
        "password": "test"
      }
    ]
  },
  "_footer": 1
}
